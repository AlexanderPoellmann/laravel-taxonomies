{
    "name": "vendocrat/laravel-taxonomies",
    "description": "Simple, nestable Terms & Taxonomies for Laravel 5.",
    "homepage" : "https://github.com/AlexanderPoellmann/laravel-taxonomies",
    "license": "MIT",
    "keywords": [
        "taxonomies",
        "laravel"
    ],
    "authors": [
        {
            "name"     : "Alexander Manfred Poellmann",
            "homepage" : "http://twitter.com/AMPoellmann",
            "role"     : "developer"
        }
    ],
    "require": {
        "php"                   : ">=5.5.9",
        "illuminate/support"    : ">=5.1",
        "illuminate/database"   : ">=5.1",
        "cviebrock/eloquent-sluggable" : "~3.0"
    },
    "require-dev": {
        "mockery/mockery" : "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "vendocrat\\Taxonomies\\" : "src/"
        }
    },
    "extra": {
        "branch-alias" : {
            "dev-master" : "0.0.x-dev"
        }
    },
    "support" : {
        "source" : "https://github.com/AlexanderPoellmann/laravel-taxonomies",
        "issues" : "https://github.com/AlexanderPoellmann/laravel-taxonomies/issues"
    }
}
